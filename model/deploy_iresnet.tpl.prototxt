layer {  name: "zoom_Concat2_itr2"  type: "Concat"  bottom: "zoom_conv1b_itr2"  bottom: "zoom_deconv2_itr2"  bottom: "zoom_upsampled_2to1_itr2"  top: "zoom_concat2_itr2"}

#predict zoom1
layer {  name: "zoom_fused1_itr2"  type: "Convolution"  bottom: "zoom_concat2_itr2"  top: "zoom_fused1_itr2"
  param {  name: "zoom_fused1_w"    lr_mult: 1    decay_mult: 0  }  param {  name: "zoom_fused1_b"    lr_mult: 1    decay_mult: 0  }
  convolution_param {    num_output: 64    pad: 1    kernel_size: 3    stride: 1    weight_filler {      type: "msra"    }    bias_filler {      type: "constant"    }    engine: CUDNN  }
}
layer {  name: "zoom_Convolution1_itr2"  type: "Convolution"  bottom: "zoom_fused1_itr2"  top: "zoom_predict1_res_itr2"
  param {  name: "zoom_predict1_w"    lr_mult: 1    decay_mult: 0  }  param {  name: "zoom_predict1_b"    lr_mult: 1    decay_mult: 0  }
  convolution_param {    num_output: 1    pad: 1    kernel_size: 3    stride: 1    weight_filler {      type: "msra"    }    bias_filler {      type: "constant"    }    engine: CUDNN  }
}

layer {  name: "zoom_deconv1_itr2"  type: "Deconvolution"  bottom: "zoom_fused1_itr2"  top: "zoom_deconv1_itr2"
  param {  name: "zoom_deconv1_w"  lr_mult: 1    decay_mult: 0  }  param {  name: "zoom_deconv1_b"   lr_mult: 0    decay_mult: 0  }
  convolution_param {    num_output: 32    pad: 1    kernel_size: 4    stride: 2    weight_filler {      type: "msra"    }    bias_filler {      type: "constant"    }    engine: CUDNN  }
}
layer {  name: "zoom_ReLU1p_itr2"  type: "ReLU"  bottom: "zoom_deconv1_itr2"  top: "zoom_deconv1_itr2"  relu_param {    negative_slope: 0.1  }}
layer {  name: "zoom_upsample_1to0_itr2"  type: "Deconvolution"  bottom: "zoom_predict1_res_itr2"  top: "zoom_upsampled_1to0_itr2"
  param {  name: "zoom_upsample1_w"  lr_mult: 1    decay_mult: 0  }  param {  name: "zoom_upsample1_b"  lr_mult: 0    decay_mult: 0  }
  convolution_param {    num_output: 1    pad: 1    kernel_size: 4    stride: 2    weight_filler {      type: "msra"    }    bias_filler {      type: "constant"    }    engine: CUDNN  }
}
layer {  name: "zoom_Concat1_itr2"  type: "Concat"  bottom: "zoom_conv0_itr2"  bottom: "zoom_deconv1_itr2"  bottom: "zoom_upsampled_1to0_itr2"  top: "zoom_concat1_itr2"}

#predict zoom0
layer {  name: "zoom_fused0_itr2"  type: "Convolution"  bottom: "zoom_concat1_itr2"  top: "zoom_fused0_itr2"
  param {  name: "zoom_fused0_w"    lr_mult: 1    decay_mult: 0  }  param {  name: "zoom_fused0_b"    lr_mult: 1    decay_mult: 0  }
  convolution_param {    num_output: 32    pad: 1    kernel_size: 3    stride: 1    weight_filler {      type: "msra"    }    bias_filler {      type: "constant"    }    engine: CUDNN  }
}
layer {  name: "zoom_Convolution0_itr2"  type: "Convolution"  bottom: "zoom_fused0_itr2"  top: "zoom_predict0_res_itr2"
  param {  name: "zoom_predict0_w"    lr_mult: 1    decay_mult: 0  }  param {  name: "zoom_predict0_b"    lr_mult: 1    decay_mult: 0  }
  convolution_param {    num_output: 1    pad: 1    kernel_size: 3    stride: 1    weight_filler {      type: "msra"    }    bias_filler {      type: "constant"    }    engine: CUDNN  }
}

layer {  name: "sumation_initial_res0_itr2"  type: "Eltwise"  bottom: "zoom_predict0n_itr1"  bottom: "zoom_predict0_res_itr2"  top: "zoom_predict0_itr2"  eltwise_param {    operation: SUM    coeff: 1.0  coeff: 1.0  }}

layer {  name: "zoom_NegReLU_dispn0_itr2"  type: "NegReLU"  bottom: "zoom_predict0_itr2"  top: "zoom_predict0n_itr2"  relu_param {    negative_slope: 0  }}


